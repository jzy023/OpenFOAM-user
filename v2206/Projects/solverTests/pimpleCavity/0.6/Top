FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=64;scalar=64";
    class       volScalarField;
    location    "0.6";
    object      Top;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 308.15;

boundaryField
{
    movingWall
    {
        type            calculated;
        value           uniform 308.15;
    }
    fixedWalls
    {
        type            calculated;
        value           uniform 308.15;
    }
    frontAndBack
    {
        type            empty;
    }
}


