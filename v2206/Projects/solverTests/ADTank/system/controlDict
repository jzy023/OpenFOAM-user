FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

// endTime         60;
endTime         200;
deltaT          1;

adjustableRunTime yes; 

writeControl    runTime;

writeInterval   20;

purgeWrite      0;

writeFormat     ascii;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   5;

runTimeModifiable true;

functions
{
    // #includeFunc mag(U)

    linesample_0
    {
        type                    sets;
        libs                    ("libsampling.so");
        writeControl            $writeControl;
        writeInterval           1;
        interpolationScheme     cell;
        setFormat               csv;
        sets
        (
            data
            {
                type            uniform;
                axis            xyz;
                start           (-0.11  0.0   0.0);
                end             (0.11   0.0   0.0);
                nPoints         500;
            }
        );
        fields (U);
    }

    linesample_1
