FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=64;scalar=64";
    class       volScalarField;
    location    "300";
    object      Sh2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 2.5055e-07;

boundaryField
{
    movingWall
    {
        type            fixedValue;
        value           uniform 2.5055e-07;
    }
    fixedWalls
    {
        type            zeroGradient;
    }
    floor
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


