    // =========================================================
	linesample_00_U
	{
		type						sets;
		libs						("libsampling.so");
		timeStart					45;
		writeControl				$writeControl;
		writeInterval				0.5;
		interpolationScheme			cell;
		setFormat					csv;
		sets						(
			data
			{
				type				uniform;
				axis				xyz;
				start				(-0.002614672282429738  -0.02988584094275236 -0.2);
				end					(-0.002614672282429738  -0.02988584094275236  0.15);
				nPoints				500;
			}
		);
		fields	(U);
	}

