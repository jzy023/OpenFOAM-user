// application     buoyantBoussinesqPimpleFoam;
// application     ADMSimpleFoam;
application     ADMFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

// endTime         5;
// deltaT          0.1;
// writeInterval   1;

endTime         200;
deltaT          0.01;
writeInterval   10;

adjustableRunTime yes; 

writeControl    runTime;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   5;

adjustTimeStep  true;

runTimeModifiable true;

maxCo           10;
// maxCo           40;

maxDeltaT       1;

